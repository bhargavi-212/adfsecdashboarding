{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "kapubhargavi2912-df"
		},
		"LS_BlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_BlobStorage'"
		},
		"LS_DataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_DataLakeStorage'"
		},
		"LS_AzureSqlDatabase_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlseconddashboarding.database.windows.net"
		},
		"LS_AzureSqlDatabase_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sqldbseconddashboarding"
		},
		"LS_AzureSqlDatabase_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "cloud"
		},
		"LS_DataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlthirdaccdashboarding.dfs.core.windows.net/"
		},
		"kvbavanasidashboarding_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kvbavanasidashboarding.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "kapubhargavi2912-df_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_BlobStorage_connectionString": {
						"value": "[parameters('LS_BlobStorage_connectionString')]"
					},
					"LS_DataLakeStorage_accountKey": {
						"value": "[parameters('LS_DataLakeStorage_accountKey')]"
					},
					"LS_DataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('LS_DataLakeStorage_properties_typeProperties_url')]"
					},
					"kvbavanasidashboarding_properties_typeProperties_baseUrl": {
						"value": "[parameters('kvbavanasidashboarding_properties_typeProperties_baseUrl')]"
					},
					"LS_AzureSqlDatabase_properties_typeProperties_server": {
						"value": "[parameters('LS_AzureSqlDatabase_properties_typeProperties_server')]"
					},
					"LS_AzureSqlDatabase_properties_typeProperties_database": {
						"value": "[parameters('LS_AzureSqlDatabase_properties_typeProperties_database')]"
					},
					"LS_AzureSqlDatabase_properties_typeProperties_userName": {
						"value": "[parameters('LS_AzureSqlDatabase_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}